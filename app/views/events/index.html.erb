<h1>Listing Events</h1>
 <%= link_to 'New event', new_user_event_path(@user) %>
<table>
  <tr>
    <th>Name</th>
    <th>Venue</th>
    <th>About</th>
    <th colspan=3></th>
  </tr>
 
  <% @events.each do |event| %>
    <tr>
      <td><%= event.name %></td>
      <td><%= event.venue %></td>
      <td><%= event.description %></td>
      <td><%= link_to 'Show', user_event_path(event.user,event) %>
      <td><%= link_to 'Edit', edit_user_event_path(event.user,event) %>
      <%= link_to 'Destroy Event', [event.user, event],
               method: :delete,
               data: { confirm: 'Are you sure?' } %></td>
      </td>
    </tr>
  <% end %>
</table>